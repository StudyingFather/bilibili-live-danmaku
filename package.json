{
  "name": "bilibili-danmaku",
  "version": "0.3.12",
  "author": "其妙 <hling51325@163.com>",
  "description": "这是一个哔哩哔哩的直播弹幕应用",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "ui": "vue ui",
    "release": "npm run build -- -p always",
    "cnpmi": "npm --registry https://registry.npm.taobao.org install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usagiring/bilibili-live-danmaku.git"
  },
  "build": {
    "productName": "bilibili-danmaku",
    "appId": "com.electron.bilibili-danmaku",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraFiles": [
      "config.yaml"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/icons"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "dependencies": {
    "@electron/remote": "^2.0.4",
    "@tokine/bilibili-bridge": "^0.0.25",
    "@tokine/bilibili-danmaku-page": "^0.1.9",
    "@tokine/bilibili-recorder": "^0.0.11",
    "axios": "^0.26.0",
    "cookie": "^0.4.2",
    "echarts": "^5.3.0",
    "echarts-wordcloud": "^2.0.0",
    "electron-updater": "^4.6.5",
    "flv.js": "^1.6.2",
    "font-list": "^1.4.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nedb": "^1.8.0",
    "pako": "^2.0.4",
    "view-design": "^4.7.0",
    "vue": "^3.2.31",
    "vue-electron": "^1.0.6",
    "vue-router": "^4.0.12",
    "vue-smooth-dnd": "^0.8.1",
    "vuex": "^4.0.2",
    "vuex-electron": "github:usagiring/vuex-electron",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-proposal-function-bind": "^7.16.7",
    "@babel/plugin-proposal-json-strings": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-proposal-throw-expressions": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "ajv": "^8.10.0",
    "babel-loader": "^8.2.3",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cfonts": "^2.10.0",
    "chalk": "^5.0.0",
    "chokidar": "^3.5.3",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "del": "^6.0.0",
    "devtron": "^1.4.0",
    "electron": "^19.0.0",
    "electron-builder": "^22.14.13",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "2.5.3",
    "multispinner": "^0.2.1",
    "node-loader": "^2.0.0",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0"
  }
}
